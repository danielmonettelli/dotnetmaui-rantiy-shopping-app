<?xml version="1.0" encoding="utf-8" ?>
<ResourceDictionary
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">

    <!--#region ShoppingCartPage-->
    <Style
        x:Key="BtnPlaceOrder"
        TargetType="Button">
        <Setter Property="FontFamily" Value="Roboto#700" />
        <Setter Property="FontSize" Value="20" />
        <Setter Property="CornerRadius" Value="8" />
        <Setter Property="BackgroundColor" Value="{AppThemeBinding Light={StaticResource primary_color_light}, Dark={StaticResource primary_color_dark}}" />
        <Setter Property="TextColor" Value="{AppThemeBinding Light={StaticResource white_color}, Dark={StaticResource text_primary_color_dark}}" />
    </Style>
    <!--#endregion-->

</ResourceDictionary>